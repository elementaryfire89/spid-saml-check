<samlp:Response
    Destination="{{AssertionConsumerURL}}"
    ID="{{ResponseID}}"
    InResponseTo="{{AuthnRequestID}}" 
    IssueInstant="{{IssueInstant}}"
    Version="2.0" xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion"
    xmlns:samlp="urn:oasis:names:tc:SAML:2.0:protocol">
